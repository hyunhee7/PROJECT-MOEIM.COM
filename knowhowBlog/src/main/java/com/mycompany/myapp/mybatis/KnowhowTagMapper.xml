<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="knowhowTag">
	<insert id="insert" parameterType="knowhowTagDto"> 
		INSERT INTO kh_tag (tag_name, tag_kh_num) 
		VALUES(#{tag_name},#{tag_kh_num}) 
	</insert>
	<select id="getTags" resultType="knowhowTagDto" parameterType="knowhowTagDto"> 
		SELECT tag_num, tag_name, tag_proj_num, tag_post_num
		FROM proj_post_tag
		WHERE tag_proj_num=#{post_proj_num} AND tag_post_num=#{post_num}
	</select>		
</mapper>


