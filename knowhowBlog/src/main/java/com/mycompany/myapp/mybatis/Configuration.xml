<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.mycompany.myapp.dto.MembersDto" alias="membersDto"/>
		<typeAlias type="com.mycompany.myapp.dto.ProjBoardDto" alias="ProjBoardDto"/>
		<typeAlias type="com.mycompany.myapp.dto.ProjTimelineDto" alias="ProjTimelineDto"/>
	</typeAliases>
	
	<!-- sql 문을 정의하고 있는 xml 문서 -->
	<mappers>		
		<mapper resource="com/mycompany/myapp/mybatis/MembersMapper.xml"/>
		<mapper resource="com/mycompany/myapp/mybatis/ProjBoardMapper.xml"/>
		<mapper resource="com/mycompany/myapp/mybatis/ProjTimelineMapper.xml"/>
	</mappers>	
</configuration>







