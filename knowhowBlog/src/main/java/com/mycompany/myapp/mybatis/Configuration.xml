<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.mycompany.myapp.dto.MembersDto" alias="membersDto"/>
		<typeAlias type="com.mycompany.myapp.dto.ProjBoardDto" alias="ProjBoardDto"/>
		<typeAlias type="com.mycompany.myapp.dto.ProjTimelineDto" alias="ProjTimelineDto"/>
		<typeAlias type="com.mycompany.myapp.dto.ProjPostTagDto" alias="ProjPostTagDto"/>
		<typeAlias type="com.mycompany.myapp.dto.KnowhowDto" alias="KnowhowDto"/>
		<typeAlias type="com.mycompany.myapp.dto.KnowhowTagDto" alias="KnowhowTagDto"/>
		<typeAlias type="com.mycompany.myapp.dto.KnowhowCommentDto" alias="KnowhowCommentDto"/>
		<typeAlias type="com.mycompany.myapp.dto.ProjPostCommentDto" alias="ProjPostCommentDto"/>
	</typeAliases>
	
	<!-- sql 문을 정의하고 있는 xml 문서 -->
	<mappers>		
		<mapper resource="com/mycompany/myapp/mybatis/MembersMapper.xml"/>
		<mapper resource="com/mycompany/myapp/mybatis/ProjBoardMapper.xml"/>
		<mapper resource="com/mycompany/myapp/mybatis/ProjTimelineMapper.xml"/>
		<mapper resource="com/mycompany/myapp/mybatis/ProjPostTagMapper.xml"/>
		<mapper resource="com/mycompany/myapp/mybatis/KnowhowMapper.xml"/>
		<mapper resource="com/mycompany/myapp/mybatis/KnowhowTagMapper.xml"/>
	</mappers>	
</configuration>







